<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jfit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jfitness.activities.InitialActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jfitness.activities.RegisterUserActivity"
            android:label="@string/title_activity_register_user" >
        </activity>
        <activity
            android:name="com.jfitness.activities.RecommendActivity"
            android:label="@string/title_activity_recommend" >
        </activity>
        <activity
            android:name="com.jfitness.activities.DashboardActivity"
            android:label="@string/title_activity_dashboard" >
        </activity>
        <activity
            android:name="com.jfitness.activities.WalkingActivity"
            android:label="@string/title_activity_walking" >
        </activity>
        <activity
            android:name="com.jfitness.activities.PhysicalActivity"
            android:label="@string/title_activity_physical" >
        </activity>
    </application>

</manifest>
